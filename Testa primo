Q3
	MOV D,15
	MOV B,2
	MOV C,2
	CMP D,3
	JZ PRIM
	CMP D,2
	JZ PRIM
LOOP:	
	MOV A,B
	MUL C
	CMP A,D
	JZ NPRIM
	INC C
	CMP C, 16
	JZ LUUP
	PUSH D
	CALL RAIZ
	CMP D,B
	JZ PRIM
	POP D
	JMP LOOP
LUUP:
	MOV C,2
	ADD B,1
	JMP LOOP

NPRIM:
	MOV [232],48
	HLT
PRIM:
	MOV [232],49
	HLT

RAIZ:
	MOV A,16
	CMP A,D
	JC R4
	MOV A,64
	CMP A,D
	JC R8
	SHR D,1
	RET
	
R4:
	SHR D,2
	INC D
	RET	
	
R8:
	SHR D,3
	INC D	
	RET
