JMP STRT
	DB "daltonli"
	DB 0
STRT:
	MOV D,2
	MOV C,232
MAIN:
	MOV B, [D]
	CMP B,97
	JZ VOG
	CMP B,101
	JZ VOG
	CMP B,105
	JZ VOG
	CMP B,111
	JZ VOG
	CMP B,117
	JZ VOG
	PUSH B
	INC D
	INC A
	CMP B,0
	JNZ MAIN
	POP B
	
CONS:
	POP B
	DEC A
	MOV [C],B
	INC C
	CMP A,1
	JNZ CONS
	HLT
VOG:
	MOV [C],B
	INC C
	INC D
	JMP MAIN  
